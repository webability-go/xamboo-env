    { "name": "cms", "source": "built-in" },
    { "name": "error", "source": "built-in" },
    { "name": "myhandler", "source": "extern", "library": "./example/components/myhandler/myhandler.so" }
  ],
  "engines": [
    {
      "name": "simple",
      "source": "built-in"
    },
    {
      "name": "library",
      "source": "built-in"
    },
    {
      "name": "template",
      "source": "built-in"
    },
    {
      "name": "language",
      "source": "built-in"
    },
    {
      "name": "wajafapp",
      "source": "built-in"
    }
  ]
