{
  "log": {
    "comments": "this field is ignored by the xamboo. The rotate field is 1d, 1w, 1m, for days, weeks, months, compress = true to compress older files. logs can be file:[file] or stdout: or stderr: or discard:. the sys log is used for deep code log (like debug, pages code log, etc). Is is recommended to let sys to stdout and log by operating system with >> ",
    "sys": "file:./example/logs/xamboo-sys.log",
    "errors": "file:./example/logs/xamboo-error.log",
    "rotate": "1d",
    "compress": true
  },

  "not-include": [
    "./master/config.json"
  ],

  "include": [
    "./example/config.json"
  ],

  "no-components": [
  ],
  "components": [
    { "name": "log", "source": "built-in" },
    { "name": "stat", "source": "built-in" },
    { "name": "redirect", "source": "built-in" },
    { "name": "auth", "source": "built-in" },
    { "name": "compress", "source": "built-in" },
    { "name": "minify", "source": "built-in" },
    { "name": "origin", "source": "built-in" },
    { "name": "fileserver", "source": "built-in" },
    { "name": "cms", "source": "built-in" },
    { "name": "error", "source": "built-in" },
    { "name": "myhandler", "source": "extern", "library": "./example/components/myhandler/myhandler.so" }
  ],
  "engines": [
    {
      "name": "simple",
      "source": "built-in"
    },
    {
      "name": "library",
      "source": "built-in"
    },
    {
      "name": "template",
      "source": "built-in"
    },
    {
      "name": "language",
      "source": "built-in"
    },
    {
      "name": "wajafapp",
      "source": "built-in"
    }
  ]
}
